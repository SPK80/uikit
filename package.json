{
  "name": "uikit",
  "version": "0.1.0",
  "license": "MIT",
  "description": "A React-based UI library",
  "main": "dist/index.cjs",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./components/Button": {
      "import": "./dist/components/Button.es.js",
      "require": "./dist/components/Button.cjs",
      "types": "./dist/components/Button.d.ts"
    },
    "./components/Input": {
      "import": "./dist/components/Input.es.js",
      "require": "./dist/components/Input.cjs",
      "types": "./dist/components/Input.d.ts"
    },
    "./components/Search": {
      "import": "./dist/components/Search.es.js",
      "require": "./dist/components/Search.cjs",
      "types": "./dist/components/Search.d.ts"
    }
  },
  "files": [
    "components"
  ],
  "scripts": {
    "build": "rollup -c"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "antd": "^5.25.3",
    "path": "^0.12.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.41.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}